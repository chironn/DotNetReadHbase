<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--Hbase地址 -->
    <add key="IPAddress" value="192.168.2.111" />
    <!--Hbase端口 -->
    <add key="Port" value="9090" />
    <!--Hbase表名 -->
    <add key="TableName" value="BYD_DAT" />
    <!--Hbase操作类型“1：查询数据拷贝至本地文件”；“2：删除指定RowKey数据并备份至本地文件”-->
    <add key="Operation" value="1" />
    <!--Hbase查询数据读本地文件地址 -->
    <add key="FileReadPath" value="C:\Users\Administrator\Desktop\1.txt" />
    <!--Hbase查询数据写本地文件地址 -->
    <add key="FileWritePath" value="C:\Users\Administrator\Desktop\RowKeyResult_" />
    <!--Hbase查询数据删除指定RowKey文件地址 -->
    <add key="FileDeleteRowKeyPath" value="C:\Users\Administrator\Desktop\RowKeyDelete.txt" />
    <!--Hbase查询数据删除RowKey备份本地文件地址 -->
    <add key="FileDeleteBackUpPath" value="C:\Users\Administrator\Desktop\RowKeyBackUp_" />
    <!--Hbase查询下载数据最大容量 -->
    <add key="HbaseDownloadDataCapacity" value="100000" />
  </appSettings>
  
</configuration>